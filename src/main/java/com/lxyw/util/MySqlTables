DROP TABLE IF EXISTS `sale_list`;
CREATE TABLE `sale_list` (
  `ID` varchar(32) NOT NULL COMMENT '主键',
  `SALE_CUSTOMER_ID` varchar(500) NOT NULL COMMENT '客户名称',
  `SALE_CUSTOMER_NAME` varchar(500) NOT NULL COMMENT '客户ID',
  `SALE_CUSTOMER_TYPE` varchar(50) NOT NULL COMMENT '客户类别',
  `SALE_CUSTOMER_CONTACT_ID` varchar(50) DEFAULT NULL COMMENT '客户方采购联系人ID',
  `SALE_CUSTOMER_CONTACT_TXT` varchar(50) DEFAULT NULL COMMENT '冗余字段，采购方联系人姓名+电话',
  `SALE_CUSTOMER_PAY_TYPE` varchar(10) DEFAULT NULL COMMENT '付款方式 1,-对公全额付款、2-对私全额付款、3-其他',
  `SALE_CUSTOMER_TRAN_PAY_TYPE` varchar(10) DEFAULT NULL COMMENT '物流付款方式 1,-客户预付、2-客户到付、3-我方预付、4-我方到付、5双方预付、6双方到付',
  `SALE_CUSTOMER_TRAN_REQURE` varchar(10) DEFAULT NULL COMMENT '收货要求：1、货到自提点2、送货上门、3客户自提、4其他',
  `SALE_CUSTOMER_TRAN_COMPANY` varchar(100) DEFAULT NULL COMMENT '物流公司',
  `SALE_CUSTOMER_TRAN_NO` varchar(100) DEFAULT NULL COMMENT '物流号',
  `SALE_CUSTOMER_TRAN_DATE` datetime DEFAULT NULL COMMENT '发货时间',
  `SALE_CUSTOMER_TRAN_ADDRESS` varchar(500) DEFAULT NULL COMMENT '收货地址',
  `SALE_CUSTOMER_TRAN_INVOICE_ADDRESS` varchar(500) DEFAULT NULL COMMENT '发票地址',
  `SALE_CUSTOMER_REMARK` varchar(1200) DEFAULT NULL COMMENT '备注',
  `SALE_CUSTOMER_NOTICE` varchar(1200) DEFAULT NULL COMMENT '免责条款',
  `SALE_CUSTOMER_TRANBACK_COMPANY` varchar(100) DEFAULT NULL COMMENT '退货物流公司',
  `SALE_CUSTOMER_TRANBACK_NO` varchar(100) DEFAULT NULL COMMENT '退货物流单号',
  `SALE_CUSTOMER_TRANBACK_DATE` datetime DEFAULT NULL COMMENT '退货时间',
  `SALE_UER_ID` varchar(40) NOT NULL COMMENT '销售姓名',
  `SALE_UER_NAME` varchar(20) DEFAULT NULL COMMENT '销售人',
  `SALE_UER_CELLPHONE` varchar(40) DEFAULT NULL COMMENT '销售人电话',
  `SALE_UER_EMAIL` varchar(250) DEFAULT NULL COMMENT '销售email',
  `SALE_UER_WECHAT` varchar(250) DEFAULT NULL COMMENT '销售微信',
  `CREATED_BY` varchar(30) DEFAULT NULL,
  `CREATED_DATE` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  `UPDATE_BY` varchar(30) DEFAULT NULL,
  `UPDATE_DATE` timestamp NOT NULL DEFAULT '0000-00-00 00:00:00',
  `VERSION` bigint(20) DEFAULT NULL,
  PRIMARY KEY (`ID`,`SALE_UER_ID`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;


DROP TABLE IF EXISTS `sale_item_list`;
CREATE TABLE `sale_item_list` (
  `ID` varchar(32) NOT NULL COMMENT '主键',
  `SALE_ITEM_PRODUCT_NAME` varchar(500) NOT NULL COMMENT '品名',
  `SALE_LIST_ID` varchar(32) NOT NULL COMMENT '关联销售主表',
  `SALE_ITEM_PRODUCT_ID` varchar(500) NOT NULL COMMENT '商品ID，关联商品库存表ID',
  `SALE_ITEM_PRODUCT_QRCODE` varchar(100) NOT NULL COMMENT '条形码',
  `SALE_ITEM_PRODUCT_FORMAT` varchar(100) NOT NULL COMMENT '规格',
  `SALE_ITEM_PRODUCT_PARAMS` varchar(200) NOT NULL COMMENT '产品的其他数据',
  `SALE_ITEM_PRODUCT_PRICE` decimal(12,4) DEFAULT NULL COMMENT '产品单价',
  `SALE_ITEM_PRODUCT_NUM` decimal(12,4) DEFAULT NULL COMMENT '订购数量（箱）',
  `SALE_ITEM_PRODUCT_PRECENT` decimal(12,4) DEFAULT NULL COMMENT '搭赠量（件）',
  `SALE_ITEM_PRODUCT_DISCOUNT` varchar(10) DEFAULT NULL COMMENT '折扣类型 0、无优惠、1折扣优惠、2抹零优惠、3免单',
  `SALE_ITEM_PRODUCT_DISCOUNT_FILL` varchar(100) NOT NULL COMMENT '优惠详情',
  `SALE_ITEM_PRODUCT_TOTAL` decimal(12,4) DEFAULT NULL COMMENT '该品类总价',
  `SALE_ITEM_PRODUCT_ACT_TOTAL` decimal(12,4) DEFAULT NULL COMMENT '实际类总价（优惠后）',
  PRIMARY KEY (`ID`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;


