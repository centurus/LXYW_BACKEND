-- ----------------------------
-- Table structure for `customer_info`
-- ----------------------------
DROP TABLE IF EXISTS `customer_info`;
CREATE TABLE `customer_info` (
  `ID` varchar(32) NOT NULL COMMENT '主键',
  `CUSTOMER_NAME` varchar(30) NOT NULL COMMENT '联系人姓名',
  `CUSTOMER_TYPE` varchar(20) NOT NULL COMMENT '0是企业客户、1是个人客户',
  `CUSTOMER_CELLPHONE` varchar(30) DEFAULT NULL COMMENT '电话',
  `CUSTOMER_EMAIL` varchar(100) DEFAULT NULL COMMENT 'EMAIL',
  `CUSTOMER_ADDRESS` varchar(500) DEFAULT NULL COMMENT '客户地址',
  `CUSTOMER_REMARK` varchar(500) DEFAULT NULL COMMENT '备注',
  `CREATED_BY` varchar(30) DEFAULT NULL,
  `CREATED_DATE` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  `UPDATE_BY` varchar(30) DEFAULT NULL,
  `UPDATE_DATE` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP,
  `VERSION` bigint(20) DEFAULT NULL,
  `STATUS` varchar(2) DEFAULT NULL COMMENT '状态：0为正常、1为冻结、2为停用',
  PRIMARY KEY (`ID`),
  UNIQUE KEY `customerName unique` (`CUSTOMER_NAME`) USING BTREE
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

-- ----------------------------
-- Records of customer_info
-- ----------------------------

-- ----------------------------
-- Table structure for `customer_links`
-- ----------------------------
DROP TABLE IF EXISTS `customer_links`;
CREATE TABLE `customer_links` (
  `ID` varchar(32) NOT NULL COMMENT '主键',
  `CUSTOMER_INFO_ID` varchar(32) NOT NULL COMMENT '关联客户表',
  `NAME` varchar(30) NOT NULL COMMENT '联系人姓名',
  `CELLPHONE` varchar(30) NOT NULL COMMENT '电话',
  `EMAIL` varchar(100) DEFAULT NULL COMMENT 'EMAIL',
  `ADDRESS` varchar(500) DEFAULT NULL COMMENT '客户地址',
  `REMARK` varchar(500) DEFAULT NULL COMMENT '备注',
  `CREATED_BY` varchar(30) DEFAULT NULL,
  `CREATED_DATE` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  `UPDATE_BY` varchar(30) DEFAULT NULL,
  `UPDATE_DATE` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP,
  `VERSION` bigint(20) DEFAULT NULL,
  `STATUS` varchar(20) DEFAULT NULL COMMENT '联系人状态、0为正常、1为不再合作',
  PRIMARY KEY (`ID`),
  KEY `CUSTOMER_INFO_ID` (`CUSTOMER_INFO_ID`),
  CONSTRAINT `customer_links_ibfk_1` FOREIGN KEY (`CUSTOMER_INFO_ID`) REFERENCES `customer_info` (`ID`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

