DROP TABLE IF EXISTS `supplier_subject`;
CREATE TABLE `supplier_subject` (
  `ID` varchar(32) NOT NULL,
  `SUPPLIER_NAME` varchar(200) NOT NULL COMMENT '供应商名称',
  `SUPPLIER_TYPE` varchar(20) NOT NULL DEFAULT '0' COMMENT '0为公司客户、1为个人客户',
  `SUPPLIER_EMAIL` varchar(70) DEFAULT NULL COMMENT '邮件',
  `SUPPLIER_LOCATION` varchar(50) DEFAULT NULL COMMENT '供货商所属地区',
  `SUPPLIER_CELLPHONE` varchar(50) DEFAULT NULL COMMENT '供货商电话',
  `SUPPLIER_ADDRESS` varchar(500) DEFAULT NULL COMMENT '供货商地址',
  `SUPPLIER_REMARK` varchar(500) DEFAULT NULL COMMENT '备注',
  `CREATED_BY` varchar(30) DEFAULT NULL,
  `CREATED_DATE` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  `UPDATE_BY` varchar(30) DEFAULT NULL,
  `UPDATE_DATE` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP,
  `VERSION` bigint(20) DEFAULT NULL,
  `STATUS` varchar(2) DEFAULT '0' COMMENT '状态：0为正常、1为冻结、2为停用',
  PRIMARY KEY (`ID`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

