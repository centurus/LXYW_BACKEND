CREATE TABLE `stock_list_fee` (
  `ID` varchar(32) NOT NULL COMMENT '主键',
  `FEE_TYPE` varchar(100) NOT NULL COMMENT '费用种类',
  `FEE_NUM` decimal(12,2) DEFAULT '0.00' COMMENT '价格',
  `STOCK_LIST_ID` varchar(32) NOT NULL COMMENT '关联库存',
  `CREATED_BY` varchar(30) DEFAULT NULL,
  `CREATED_DATE` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  `UPDATE_BY` varchar(30) DEFAULT NULL,
  `UPDATE_DATE` timestamp NOT NULL DEFAULT '0000-00-00 00:00:00',
  `VERSION` bigint(20) DEFAULT NULL,
  PRIMARY KEY (`ID`),
  KEY `STOCK_LIST_ID` (`STOCK_LIST_ID`),
  CONSTRAINT `stock_list_fee_ibfk_1` FOREIGN KEY (`STOCK_LIST_ID`) REFERENCES `stock_list` (`ID`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

CREATE TABLE `customer_info` (
  `ID` varchar(32) NOT NULL COMMENT '主键',
  `CUSTOMER_NAME` varchar(30) NOT NULL COMMENT '联系人姓名',
  `CUSTOMER_TYPE` varchar(20) NOT NULL COMMENT '0是企业客户、1是个人客户',
  `CUSTOMER_STATE` varchar(20) DEFAULT NULL COMMENT '联系人状态、0为正常、1为不再合作',
  `CUSTOMER_CELLPHONE` varchar(30) DEFAULT NULL COMMENT '电话',
  `CUSTOMER_EMAIL` varchar(100) DEFAULT NULL COMMENT 'EMAIL',
  `CUSTOMER_ADDRESS` varchar(500) DEFAULT NULL COMMENT '客户地址',
  `CUSTOMER_REMARK` varchar(500) DEFAULT NULL COMMENT '备注',
  `CREATED_BY` varchar(30) DEFAULT NULL,
  `CREATED_DATE` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  `UPDATE_BY` varchar(30) DEFAULT NULL,
  `UPDATE_DATE` timestamp NOT NULL DEFAULT '0000-00-00 00:00:00',
  `VERSION` bigint(20) DEFAULT NULL,
  PRIMARY KEY (`ID`),
  UNIQUE KEY `customerName unique` (`CUSTOMER_NAME`) USING BTREE
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

CREATE TABLE `customer_links` (
  `ID` varchar(32) NOT NULL COMMENT '主键',
  `CUSTOMER_INFO_ID` varchar(32) NOT NULL COMMENT '关联客户表',
  `NAME` varchar(30) NOT NULL COMMENT '联系人姓名',
  `STATE` varchar(20) DEFAULT NULL COMMENT '联系人状态、0为正常、1为不再合作',
  `CELLPHONE` varchar(30) NOT NULL COMMENT '电话',
  `EMAIL` varchar(100) DEFAULT NULL COMMENT 'EMAIL',
  `ADDRESS` varchar(500) DEFAULT NULL COMMENT '客户地址',
  `REMARK` varchar(500) DEFAULT NULL COMMENT '备注',
  `CREATED_BY` varchar(30) DEFAULT NULL,
  `CREATED_DATE` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  `UPDATE_BY` varchar(30) DEFAULT NULL,
  `UPDATE_DATE` timestamp NOT NULL DEFAULT '0000-00-00 00:00:00',
  `VERSION` bigint(20) DEFAULT NULL,
  PRIMARY KEY (`ID`),
  KEY `CUSTOMER_INFO_ID` (`CUSTOMER_INFO_ID`),
  CONSTRAINT `customer_links_ibfk_1` FOREIGN KEY (`CUSTOMER_INFO_ID`) REFERENCES `customer_info` (`ID`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;


CREATE TABLE `stock_list` (
  `ID` varchar(32) COLLATE utf8_unicode_ci NOT NULL COMMENT '主键',
  `STOCK_PRODUCT_NAME` varchar(500) COLLATE utf8_unicode_ci NOT NULL COMMENT '商品名称',
  `STOCK_PRODUCT_QNO` varchar(100) COLLATE utf8_unicode_ci NOT NULL COMMENT '商品条形码',
  `STOCK_PRODUCT_LOCATION` varchar(50) COLLATE utf8_unicode_ci NOT NULL COMMENT '产地',
  `STOCK_PRODUCT_NUM` decimal(12,2) NOT NULL COMMENT '进货数量',
  `STOCK_PRODUCT_POSITION` varchar(10) COLLATE utf8_unicode_ci NOT NULL COMMENT '单位',
  `STOCK_PRODUCT_PLUS_NUM` decimal(12,2) DEFAULT '0.00' COMMENT '散货量',
  `STOCK_PRODUCT_PLUS_POSITION` varchar(10) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT '散货单位',
  `STOCK_PRODUCT_FORMAT_NUM` decimal(4,2) NOT NULL COMMENT '每件含单件个数',
  `STOCK_PRODUCT_GUARANTEE_TIME` varchar(20) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT '保质期',
  `STOCK_PRODUCT_SINGLE_NETWEIGHT` decimal(12,2) DEFAULT NULL COMMENT '单件净含量',
  `STOCK_PRODUCT_SINGLE_CAPACITY` decimal(12,2) DEFAULT NULL COMMENT '单件体积',
  `STOCK_PRODUCT_BATCH` varchar(30) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT '批次号，可能同种商品不同批次',
  `STOCK_PRODUCT_STATE` varchar(20) COLLATE utf8_unicode_ci NOT NULL COMMENT '库存状态：0在途可预售、1在售、2已售罄、3退货在途、4已退货',
  `STOCK_PRODUCT_STORAGE` varchar(30) COLLATE utf8_unicode_ci DEFAULT NULL COMMENT '库存地',
  `STOCK_PRODUCT_WASTAGE` decimal(12,4) DEFAULT NULL COMMENT '损耗个数',
  `STOCK_PRODUCT_STORAGE_FEE` decimal(12,4) DEFAULT NULL COMMENT '仓储费',
  `STOCK_PRODUCT_SUPPLIER_SUBJECT_ID` varchar(32) COLLATE utf8_unicode_ci NOT NULL COMMENT '关联供应商',
  `PRODUCE_DATE` timestamp NOT NULL DEFAULT '0000-00-00 00:00:00' COMMENT '生产日期',
  `EXPIRE_DATE` timestamp NULL DEFAULT '0000-00-00 00:00:00' COMMENT '到期日',
  `CREATED_BY` varchar(30) COLLATE utf8_unicode_ci DEFAULT NULL,
  `CREATED_DATE` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  `UPDATE_BY` varchar(30) COLLATE utf8_unicode_ci DEFAULT NULL,
  `UPDATE_DATE` timestamp NOT NULL DEFAULT '0000-00-00 00:00:00',
  `VERSION` bigint(20) DEFAULT NULL,
  PRIMARY KEY (`ID`),
  KEY `STOCK_PRODUCT_SUPPLIER_SUBJECT_ID` (`STOCK_PRODUCT_SUPPLIER_SUBJECT_ID`),
  CONSTRAINT `stock_list_ibfk_1` FOREIGN KEY (`STOCK_PRODUCT_SUPPLIER_SUBJECT_ID`) REFERENCES `supplier_subject` (`ID`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci



CREATE TABLE `supplier_individual` (
  `ID` varchar(32) NOT NULL COMMENT '主键',
  `SUPPLIER_SUBJECT_ID` varchar(32) NOT NULL COMMENT '关联供货商主键',
  `NAME` varchar(30) NOT NULL COMMENT '联系人姓名',
  `POSITION` varchar(20) DEFAULT NULL COMMENT '职位',
  `STATE` varchar(20) DEFAULT NULL COMMENT '联系人状态、0为正常、1为不再合作',
  `CELLPHONE` varchar(30) DEFAULT NULL COMMENT '电话',
  `EMAIL` varchar(100) DEFAULT NULL COMMENT 'EMAIL',
  `REMARK` varchar(500) DEFAULT NULL COMMENT '备注',
  `CREATED_BY` varchar(30) DEFAULT NULL,
  `CREATED_DATE` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  `UPDATE_BY` varchar(30) DEFAULT NULL,
  `UPDATE_DATE` timestamp NOT NULL DEFAULT '0000-00-00 00:00:00',
  `VERSION` bigint(20) DEFAULT NULL,
  PRIMARY KEY (`ID`),
  KEY `SUPPLIER_SUBJECT_ID` (`SUPPLIER_SUBJECT_ID`),
  CONSTRAINT `supplier_individual_ibfk_1` FOREIGN KEY (`SUPPLIER_SUBJECT_ID`) REFERENCES `supplier_subject` (`ID`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

CREATE TABLE `supplier_subject` (
  `ID` varchar(32) NOT NULL,
  `SUPPLIER_NAME` varchar(200) NOT NULL COMMENT '供应商名称',
  `SUPPLIER_TYPE` varchar(20) NOT NULL DEFAULT '0' COMMENT '0为公司客户、1为个人客户',
  `SUPPLIER_EMAIL` varchar(70) DEFAULT NULL COMMENT '邮件',
  `SUPPLIER_LOCATION` varchar(50) DEFAULT NULL COMMENT '供货商所属地区',
  `SUPPLIER_PHONE` varchar(50) DEFAULT NULL COMMENT '供货商电话',
  `SUPPLIER_ADDRESS` varchar(500) DEFAULT NULL COMMENT '供货商地址',
  `SUPPLIER_STATE` varchar(20) NOT NULL DEFAULT '0' COMMENT '供货商状态，0为合作、1为不再合作',
  `SUPPLIER_REMARK` varchar(500) DEFAULT NULL COMMENT '备注',
  `CREATED_BY` varchar(30) DEFAULT NULL,
  `CREATED_DATE` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  `UPDATE_BY` varchar(30) DEFAULT NULL,
  `UPDATE_DATE` timestamp NOT NULL DEFAULT '0000-00-00 00:00:00',
  `VERSION` bigint(20) DEFAULT NULL,
  PRIMARY KEY (`ID`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

CREATE TABLE `user_info` (
  `ID` varchar(32) NOT NULL COMMENT '主键',
  `USERNAME` varchar(30) NOT NULL COMMENT '用户名',
  `PASSWORD` varchar(30) NOT NULL COMMENT '密码',
  `NAME` varchar(30) NOT NULL COMMENT '姓名',
  `STATE` varchar(2) DEFAULT '0' COMMENT '状态：0为正常、1为冻结、2为停用',
  `SEX` char(2) NOT NULL COMMENT '性别 F为女性、M为男性',
  `BIRTHDAY` date DEFAULT NULL COMMENT '生日',
  `GREDUATE_FROM` varchar(100) DEFAULT NULL COMMENT '毕业院校',
  `GREDUATE_LEVEL` varchar(10) DEFAULT NULL COMMENT '学历层次',
  `IDCARD_NO` varchar(18) DEFAULT NULL COMMENT '身份证号码',
  `CELLPHONE_NO` varchar(24) DEFAULT NULL COMMENT '手机',
  `WECHAT` varchar(100) DEFAULT NULL COMMENT '微信号',
  `ADDRESS` varchar(300) DEFAULT NULL COMMENT '地址',
  `POST_CODE` varchar(20) DEFAULT NULL COMMENT '邮编',
  `CREATED_BY` varchar(30) DEFAULT NULL,
  `CREATED_DATE` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  `UPDATE_BY` varchar(30) DEFAULT NULL,
  `UPDATE_DATE` timestamp NOT NULL DEFAULT '0000-00-00 00:00:00',
  `VERSION` bigint(20) DEFAULT NULL,
  PRIMARY KEY (`ID`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

