DROP TABLE IF EXISTS `supplier_individual`;
CREATE TABLE `supplier_individual` (
  `ID` varchar(32) NOT NULL COMMENT '主键',
  `SUPPLIER_SUBJECT_ID` varchar(32) NOT NULL COMMENT '关联供货商主键',
  `NAME` varchar(30) NOT NULL COMMENT '联系人姓名',
  `POSITION` varchar(20) DEFAULT NULL COMMENT '职位',
  `CELLPHONE` varchar(30) DEFAULT NULL COMMENT '电话',
  `EMAIL` varchar(100) DEFAULT NULL COMMENT 'EMAIL',
  `REMARK` varchar(500) DEFAULT NULL COMMENT '备注',
  `CREATED_BY` varchar(30) DEFAULT NULL,
  `CREATED_DATE` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  `UPDATE_BY` varchar(30) DEFAULT NULL,
  `UPDATE_DATE` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP,
  `VERSION` bigint(20) DEFAULT NULL,
  `STATUS` varchar(2) DEFAULT '0' COMMENT '状态：0为正常、1为冻结、2为停用',
  PRIMARY KEY (`ID`),
  KEY `SUPPLIER_SUBJECT_ID` (`SUPPLIER_SUBJECT_ID`),
  CONSTRAINT `supplier_individual_ibfk_1` FOREIGN KEY (`SUPPLIER_SUBJECT_ID`) REFERENCES `supplier_subject` (`ID`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

