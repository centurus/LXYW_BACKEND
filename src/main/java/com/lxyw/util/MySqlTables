-- ----------------------------
-- Table structure for `stock_list`
-- ----------------------------
DROP TABLE IF EXISTS `stock_list`;
CREATE TABLE `stock_list`  (
  `ID` varchar(32) CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL COMMENT '主键',
  `STOCK_PRODUCT_NAME` varchar(500) CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL COMMENT '商品名称',
  `STOCK_PRODUCT_QNO` varchar(100) CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL COMMENT '商品条形码',
  `STOCK_PRODUCT_LOCATION` varchar(50) CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL COMMENT '产地',
  `STOCK_PRODUCT_NUM` decimal(12, 2) NOT NULL COMMENT '进货数量',
  `STOCK_PRODUCT_POSITION` varchar(10) CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL COMMENT '单位',
  `STOCK_PRODUCT_PLUS_NUM` decimal(12, 2) NULL DEFAULT 0.00 COMMENT '散货量',
  `STOCK_PRODUCT_PLUS_POSITION` varchar(10) CHARACTER SET utf8 COLLATE utf8_general_ci NULL DEFAULT NULL COMMENT '散货单位',
  `STOCK_PRODUCT_FORMAT_NUM` decimal(4, 2) NOT NULL COMMENT '每件含单件个数',
  `STOCK_PRODUCT_GUARANTEE_TIME` varchar(20) CHARACTER SET utf8 COLLATE utf8_general_ci NULL DEFAULT NULL COMMENT '保质期',
  `STOCK_PRODUCT_SINGLE_NETWEIGHT` decimal(12, 2) NULL DEFAULT NULL COMMENT '单件净含量',
  `STOCK_PRODUCT_SINGLE_CAPACITY` decimal(12, 2) NULL DEFAULT NULL COMMENT '单件体积',
  `STOCK_PRODUCT_BATCH` varchar(30) CHARACTER SET utf8 COLLATE utf8_general_ci NULL DEFAULT NULL COMMENT '批次号，可能同种商品不同批次',
  `STOCK_PRODUCT_STATUS` varchar(20) CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL COMMENT '库存状态：0在途可预售、1在售、2已售罄、3退货在途、4已退货',
  `STOCK_PRODUCT_STORAGE` varchar(30) CHARACTER SET utf8 COLLATE utf8_general_ci NULL DEFAULT NULL COMMENT '库存地',
  `STOCK_PRODUCT_WASTAGE` decimal(12, 4) NULL DEFAULT NULL COMMENT '损耗个数',
  `STOCK_PRODUCT_STORAGE_FEE` decimal(12, 4) NULL DEFAULT NULL COMMENT '仓储费',
  `STOCK_PRODUCT_SUPPLIER_SUBJECT_ID` varchar(32) CHARACTER SET utf8 COLLATE utf8_general_ci NOT NULL COMMENT '关联供应商',
  `STOCK_MANUFACTURE_DATE` date NOT NULL COMMENT '生产日期',
  `STOCK_EXPIRE_DATE` date NOT NULL COMMENT '到期日',
  `CREATED_BY` varchar(30) CHARACTER SET utf8 COLLATE utf8_general_ci NULL DEFAULT NULL,
  `CREATED_DATE` timestamp(0) NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP(0),
  `UPDATE_BY` varchar(30) CHARACTER SET utf8 COLLATE utf8_general_ci NULL DEFAULT NULL,
  `UPDATE_DATE` timestamp(0) NOT NULL DEFAULT CURRENT_TIMESTAMP,
  `VERSION` bigint(20) NULL DEFAULT NULL,
  PRIMARY KEY (`ID`) USING BTREE,
  INDEX `STOCK_PRODUCT_SUPPLIER_SUBJECT_ID`(`STOCK_PRODUCT_SUPPLIER_SUBJECT_ID`) USING BTREE,
  CONSTRAINT `stock_list_ibfk_1` FOREIGN KEY (`STOCK_PRODUCT_SUPPLIER_SUBJECT_ID`) REFERENCES `db_lxyw`.`supplier_subject` (`ID`) ON DELETE RESTRICT ON UPDATE RESTRICT
) ENGINE = InnoDB CHARACTER SET = utf8 COLLATE = utf8_general_ci ROW_FORMAT = Dynamic;

-- ----------------------------
-- Records of stock_list
-- ----------------------------

