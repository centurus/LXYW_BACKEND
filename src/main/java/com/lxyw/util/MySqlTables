DROP TABLE IF EXISTS `user_info`;
CREATE TABLE `user_info` (
  `ID` varchar(32) NOT NULL COMMENT '主键',
  `USERNAME` varchar(30) NOT NULL COMMENT '用户名',
  `PASSWORD` varchar(50) NOT NULL COMMENT '密码',
  `NAME` varchar(30) NOT NULL COMMENT '姓名',
  `SEX` char(2) NOT NULL COMMENT '性别 F为女性、M为男性',
  `BIRTHDAY` date DEFAULT NULL COMMENT '生日',
  `GREDUATE_FROM` varchar(100) DEFAULT NULL COMMENT '毕业院校',
  `GREDUATE_LEVEL` varchar(10) DEFAULT NULL COMMENT '学历层次',
  `IDCARD_NO` varchar(18) DEFAULT NULL COMMENT '身份证号码',
  `CELLPHONE` varchar(24) DEFAULT NULL COMMENT '手机',
  `WECHAT` varchar(100) DEFAULT NULL COMMENT '微信号',
  `ADDRESS` varchar(300) DEFAULT NULL COMMENT '地址',
  `POST_CODE` varchar(20) DEFAULT NULL COMMENT '邮编',
  `CREATED_BY` varchar(30) DEFAULT NULL,
  `CREATED_DATE` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  `UPDATE_BY` varchar(30) DEFAULT NULL,
  `UPDATE_DATE` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP,
  `VERSION` bigint(20) DEFAULT NULL,
  `EMAIL` varchar(50) DEFAULT NULL,
  `STATUS` varchar(2) DEFAULT '0' COMMENT '状态：0为正常、1为冻结、2为停用',
  PRIMARY KEY (`ID`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

